fn main() -> Int
{
    Int x = 50;

    dumpFile("LICENSE.md");
    print(1, "factorial(");
    print(1, x);
    print(1, ") = ");
    println(1, factorial(x));
    ret 0;
}

fn dumpFile ([Char]: path)
{
    println(1, rand('a', 'z'));
    Int fd = open_r(path);
    
    if (isOpen(fd) == False) {
        exit(1);
    }
    while (isEOF(fd) == False) {
        println(1, readln(fd));
    }
    /* seek_e(fd, 0 - tell(fd));
    while (isEOF(fd) == False) {
        println(1, readln(fd));
    } */
    close(fd);
    fd = open_r("/dev/urandom");
    for (Int i = 0; i < 5; i++) {
        println(1, readln(fd));
    }
    close(fd);
}

fn factorial(Int: x) -> Int
{
    if (x == 1) {
        ret 1;
    }
    ret x * factorial(x - 1);
}
