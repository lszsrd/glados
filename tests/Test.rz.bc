FUNC foo 1
PUSH_INT 42
STORE y
LOAD x
PUSH_INT 0
EQ
JMP_IF_FALSE endif
PUSH_INT 0
RET
LABEL endif
LOAD y
LOAD x
DIV
RET
ENDFUNC

FUNC bar 1
PUSH_INT 1
STORE y
LABEL for_start
LOAD x
LOAD y
LT
JMP_IF_FALSE for_end
LOAD y
CALL foo 1
PUSH_INT 0
EQ
JMP_IF_FALSE endif
PUSH_INT 0
RET
LABEL endif
LOAD y
PUSH_INT 1
ADD
STORE y
#JMP for_start
LABEL for_end
LOAD y
RET
ENDFUNC

FUNC baz 0
PUSH_INT 100
STORE x
CALL foo 1
CALL bar 1
RET
ENDFUNC
